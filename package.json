{
   "name": "ratemycourse-backend",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/node": "^8.0.29"
   },
   "dependencies": {
      "body-parser": "^1.18.1",
      "cors": "^2.8.5",
      "express": "^4.15.4",
      "mongodb": "^3.0.8",
      "morgan": "^1.10.0",
      "nodemon": "^2.0.7",
      "reflect-metadata": "^0.1.10",
      "ts-node": "^9.1.1",
      "typeorm": "^0.2.28",
      "typescript": "^4.2.4",
      "yup": "^0.32.9"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "build": "tsc",
      "dev": "nodemon"
   },
   "engines": {
      "node": "14.x"
   },
   "nodemonConfig": {
      "ignore": [
         "**/*.test.ts",
         "**/*.spec.ts",
         ".git",
         "node_modules"
      ],
      "watch": [
         "src/**/*"
      ],
      "exec": "ts-node src/index.ts",
      "ext": "ts, js"
   }
}
